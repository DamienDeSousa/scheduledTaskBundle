{
    "name": "dades/scheduledtask",
    "license": "MIT",
    "description": "The Symfony Bundle to schedule tasks",
    "keywords": [
        "cron",
        "schedule",
        "multiplatforme",
        "Windows",
        "Linux",
        "Symfony"
    ],
    "authors": [
        {
            "name": "Damien DE SOUSA",
            "email": "desousadamien30@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.0",
        "symfony/framework-bundle": "^3.4",
        "doctrine/doctrine-bundle": "^1.3",
        "symfony/console": "^2.8 || ^3.0 || ^4.0",
        "dragonmantank/cron-expression": "^2.3",
        "doctrine/orm": "^2.5",
        "symfony/process": "^5.0"
    },
    "require-dev": {
        "symfony/phpunit-bridge": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Dades\\ScheduledTaskBundle\\":""
        }
   },
   "autoload-dev": {
        "psr-4": {
            "Dades\\ScheduledTaskBundle\\Tests\\": "Tests/"
        }
    }
}
